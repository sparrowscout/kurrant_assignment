{
  "compilerOptions": {
    "lib": ["esnext", "dom"],
    "sourceMap": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "ES6",
    "target": "ES6",
    "jsx": "preserve",
    "allowJs": false,
    "moduleResolution": "node"
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
